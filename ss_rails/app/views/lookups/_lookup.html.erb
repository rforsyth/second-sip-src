<div class="list_item">
  <span class="item_name">
    <%= link_to lookup.name, lookup_path(lookup) %>
  </span>
  (<%= "#{lookup.entity_type} #{Enums::LookupType.name(lookup.lookup_type)}" %>)
  <span class="username">
    <%= link_to lookup.owner.username, lookup.owner %>
  </span>
  <p><%= truncate(lookup.description, :length => 150) %></p>
</div>