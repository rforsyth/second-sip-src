<h1>Listing admin_tags</h1>

<table>
  <tr>
    <th>Creator</th>
    <th>Updater</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_tags.each do |admin_tag| %>
  <tr>
    <td><%= admin_tag.creator_id %></td>
    <td><%= admin_tag.updater_id %></td>
    <td><%= admin_tag.name %></td>
    <td><%= link_to 'Show', admin_tag %></td>
    <td><%= link_to 'Edit', edit_admin_tag_path(admin_tag) %></td>
    <td><%= link_to 'Destroy', admin_tag, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

