<h1>Listing reference_producers</h1>

<table>
  <tr>
    <th>Creator</th>
    <th>Updater</th>
    <th>Type</th>
    <th>Website url</th>
    <th>Name</th>
    <th>Canonical name</th>
    <th>Description</th>
    <th>Cases per year</th>
    <th>Capacity in barrels</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @reference_producers.each do |reference_producer| %>
  <tr>
    <td><%= reference_producer.creator_id %></td>
    <td><%= reference_producer.updater_id %></td>
    <td><%= reference_producer.type %></td>
    <td><%= reference_producer.website_url %></td>
    <td><%= reference_producer.name %></td>
    <td><%= reference_producer.canonical_name %></td>
    <td><%= reference_producer.description %></td>
    <td><%= reference_producer.cases_per_year %></td>
    <td><%= reference_producer.capacity_in_barrels %></td>
    <td><%= link_to 'Show', reference_producer %></td>
    <td><%= link_to 'Edit', edit_reference_producer_path(reference_producer) %></td>
    <td><%= link_to 'Destroy', reference_producer, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

